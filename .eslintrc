{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "jest": true
  },
  "rules": {
    "comma-dangle": [
      "error",
      "never"
    ],
    "import/no-extraneous-dependencies": 0,
    "linebreak-style": 0,
    "react/destructuring-assignment": [
      1,
      "never"
    ],
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "react/prefer-stateless-function": [0]
  }
}